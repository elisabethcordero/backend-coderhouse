<div class="productList">
    {{#each products}}
    <div class="productCard">
        <div class="cardProduct_image">
            <img src="{{this.thumbnail}}" alt="{{this.name}}">
        </div>
        <div class="cardProduct_info">
            <h3>{{this.name}}</h3>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.code}}</p>
        </div>
    </div>
    {{/each}}
</div>